   "name": "Spam Goormet",
   "version": "1.1.2",
   "manifest_version": 2,
   "description": "Google Chrome/Chromium extension to ease the use of spamgourmet.com",
   "icons": {
      "16": "goormet16.png",
      "48": "goormet48.png",
      "128": "goormet128.png"
   },
   "background": {
      "page": "background.html"
   },
   "permissions": [
      "clipboardWrite",
      "contextMenus",
      "tabs",
      "http://*/*",
      "https://*/*"
   ],
   // "__words_data": "words.js",
   "options_page": "options.html"
}
